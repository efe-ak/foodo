		"object-merge": "~2.5.1",
		"pretty-hrtime": "^0.2.2",
		"shimbro": "0.0.11",
		"swig": "^1.4.2",
		"swig-highlight": "^0.2.0",
		"time-grunt": "^1.0.0",
		"vinyl-fs": "^0.3.9",
		"vinyl-source-stream": "^1.0.0",
		"watchify": "^2.0.0",
		"wiredep": "^1.8.6"
	},
	"browserify": {
		"transform": [
			"debowerify",
			"browserify-shim"
		]
	},
	"browserify-shim": {
		"jquery": "global:jQuery",
		"bootstrap": {
			"depends": [
				"jquery:jQuery"
			]
		},
		"holderjs": "global:Holder",
		"jquery.breakpoints": "jquery.breakpoints",
		"raphael": "global:Raphael",
		"mocha": "mocha",
		"morrisjs": {
			"depends": [
				"jquery:jQuery",
				"raphael:Raphael",
				"mocha"
			],
			"exports": "global:Morris"
		},
		"jquery.nicescroll": "jquery.nicescroll",
		"datatables": {
			"depends": [
				"jquery:jQuery"
			]
		},
		"datatables-plugins": "datatables-plugins",
		"Grid-A-Licious": "Grid-A-Licious",
		"bootstrap-select": {
			"depends": [
				"jquery:jQuery"
			]
		},
		"jquery.cookie": {
			"depends": [
				"jquery:jQuery"
			]
		},
		"bootstrap3-datepicker": {
			"depends": [
				"jquery:jQuery",
				"bootstrap"
			]
		},
		"jquery-minicolors": "jquery-minicolors",
		"modernizr": "global:Modernizr",
		"jquery-bridget": "jquery-bridget",
		"seiyria-bootstrap-slider": {
			"depends": [
				"jquery:jQuery",
				"bootstrap",
				"modernizr:Modernizr",
				"jquery-bridget"
			]
		},
		"handlebars": "global:Handlebars"
	},
	"browser": {
		"bootstrap": "./bower_components/bootstrap/dist/js/bootstrap.js",
		"jquery.breakpoints": "./bower_components/jquery.breakpoints/breakpoints.js",
		"mocha": "./bower_components/mocha/mocha.js",
		"jquery.nicescroll": "./bower_components/jquery.nicescroll/jquery.nicescroll.js",
		"datatables": "./bower_components/datatables/media/js/jquery.dataTables.js",
		"datatables-plugins": "./bower_components/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.js",
		"Grid-A-Licious": "./bower_components/Grid-A-Licious/jquery.grid-a-licious.js",
		"bootstrap-select": "./bower_components/bootstrap-select/dist/js/bootstrap-select.js",
		"jquery.cookie": "./bower_components/jquery.cookie/jquery.cookie.js",
		"bootstrap3-datepicker": "./bower_components/bootstrap3-datepicker/js/bootstrap-datepicker.js",
		"jquery-minicolors": "./bower_components/jquery-minicolors/jquery.minicolors.js",
		"jquery-bridget": "./bower_components/jquery-bridget/jquery.bridget.js",
		"seiyria-bootstrap-slider": "./bower_components/seiyria-bootstrap-slider/js/bootstrap-slider.js"
	},
	"dependencies": {
		"grunt-parallel": "~0.3.1"
	}
}
